<?xml version="1.0" encoding="utf-8"?>
<com.jackleeentertainment.oq.ui.widget.VerticalScrollview
    xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/cv_people__frag_newoq_0"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            layout="@layout/cardview_titlelistaction"/>


        <include
            android:id="@+id/cv_sumtype__frag_newoq_0"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            layout="@layout/cardview_titleedittextillust"/>


        <include
            android:id="@+id/cv_breakdown__frag_newoq_0"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            layout="@layout/cardview_titlelistaction"/>

        <include
            android:id="@+id/tv_done__frag_newoq_0"
            android:layout_height="56dip"
            android:layout_width="match_parent"
            layout="@layout/tv_done"/>


    </LinearLayout>


</com.jackleeentertainment.oq.ui.widget.VerticalScrollview>